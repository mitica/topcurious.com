---
permalink: /{{ locale }}/search.json
eleventyExcludeFromCollections: true
---
{"search" : [
{%- for item in collections['posts_' + locale] -%}
  {
    "url" : "{{ item.url }}",
    "title" : "{{ item.data.title | trim }}"
  }{% if not loop.last %},{% else %}{%- endif -%}
{%- endfor -%}
]}